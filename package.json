{
  "name": "google-tag-manager-mcp-server",
  "version": "3.0.4",
  "description": "MCP server for Google Tag Manager",
  "license": "Apache-2.0",
  "author": "stape-io",
  "homepage": "https://github.com/stape-io/google-tag-manager-mcp-server",
  "main": "dist/index.js",
  "bin": {
    "google-tag-manager-mcp-server": "./dist/index.js",
    "gtm-mcp-local": "./dist/index-local.js"
  },
  "keywords": [
    "mcp",
    "modelcontextprotocol",
    "gtm",
    "stape"
  ],
  "scripts": {
    "build": "echo 'Using ts-node for local execution, no build needed'",
    "build:local": "echo 'Using ts-node for local execution, no build needed'",
    "build:cloudflare": "tsc --skipLibCheck && chmod 755 dist/index.js",
    "postinstall": "",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext .ts src --fix",
    "prepublish": "",
    "deploy": "npm run build:cloudflare && wrangler deploy",
    "dev:cloudflare": "wrangler dev",
    "cf-typegen": "wrangler types --include-env=false",
    "auth": "ts-node --transpileOnly src/authenticate.ts",
    "local": "ts-node --transpileOnly src/index-local.ts",
    "local:dev": "ts-node-dev --respawn --transpile-only src/index-local.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.1",
    "dotenv": "^16.3.1",
    "google-auth-library": "^9.4.1",
    "googleapis": "^128.0.0",
    "open": "^10.0.3",
    "zod": "^3.22.4"
  },
  "optionalDependencies": {
    "@cloudflare/workers-oauth-provider": "^0.0.10",
    "agents": "^0.2.0",
    "hono": "^4.9.8",
    "wrangler": "^4.16.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.30.1"
  }
}
